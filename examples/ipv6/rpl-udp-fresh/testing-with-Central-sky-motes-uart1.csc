      <source EXPORT="discard">[CONTIKI_DIR]/examples/ipv6/coral-pp-testing/udp-server.c</source>
      <commands EXPORT="discard">make udp-server.sky TARGET=sky</commands>
      <firmware EXPORT="copy">[CONTIKI_DIR]/examples/ipv6/coral-pp-testing/udp-server.sky</firmware>
